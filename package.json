{
  "name": "dashboard",
  "version": "0.0.1",
  "scripts": {
    "dev": "yarn theme && vite dev",
    "build": "yarn theme && vite build",
    "build:sb": "yarn theme && storybook build",
    "preview": "yarn theme && vite preview",
    "lint": "eslint",
    "theme": "smui-theme compile static/smui.css -i src/globalStyles",
    "test": "vitest",
    "upgrade:core": "yarn up '@aneuhold/*'",
    "upgrade:smui": "yarn up '@smui/*'",
    "storybook": "yarn theme && storybook dev -p 6006",
    "sub": "yarn sub:core-ts-api-lib",
    "unsub": "yarn unsub:core-ts-api-lib",
    "sub:core-ts-api-lib": "local-npm subscribe @aneuhold/core-ts-api-lib",
    "sub:eslint-config": "local-npm subscribe @aneuhold/eslint-config",
    "unsub:core-ts-api-lib": "local-npm unsubscribe @aneuhold/core-ts-api-lib",
    "unsub:eslint-config": "local-npm unsubscribe @aneuhold/eslint-config"
  },
  "packageManager": "yarn@4.5.1",
  "type": "module",
  "devDependencies": {
    "@aneuhold/core-ts-api-lib": "^2.2.7",
    "@aneuhold/core-ts-lib": "^2.3.8",
    "@aneuhold/eslint-config": "^1.3.15",
    "@aneuhold/local-npm-registry": "^0.2.15",
    "@fontsource/fira-mono": "^5.1.0",
    "@neoconfetti/svelte": "^2.2.1",
    "@smui-extra/accordion": "^7.0.0",
    "@smui-extra/autocomplete": "^7.0.0",
    "@smui/button": "^7.0.0",
    "@smui/card": "^7.0.0",
    "@smui/checkbox": "^7.0.0",
    "@smui/chips": "^7.0.0",
    "@smui/circular-progress": "^7.0.0",
    "@smui/common": "^7.0.0",
    "@smui/dialog": "^7.0.0",
    "@smui/drawer": "^7.0.0",
    "@smui/fab": "^7.0.0",
    "@smui/form-field": "^7.0.0",
    "@smui/icon-button": "^7.0.0",
    "@smui/list": "^7.0.0",
    "@smui/menu": "^7.0.0",
    "@smui/menu-surface": "^7.0.0",
    "@smui/paper": "^7.0.0",
    "@smui/segmented-button": "^7.0.0",
    "@smui/select": "^7.0.0",
    "@smui/snackbar": "^7.0.0",
    "@smui/textfield": "^7.0.0",
    "@smui/tooltip": "^7.0.0",
    "@smui/top-app-bar": "^7.0.0",
    "@storybook/addon-actions": "^8.3.6",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-interactions": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/blocks": "^8.3.6",
    "@storybook/svelte": "^8.3.6",
    "@storybook/sveltekit": "^8.3.6",
    "@storybook/test": "^8.3.6",
    "@sveltejs/adapter-static": "^3.0.5",
    "@sveltejs/kit": "^2.6.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/cookie": "^0.6.0",
    "@types/node": "^20.17.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "bson": "^7.0.0",
    "eslint": "^9.14.0",
    "prettier": "^3.3.3",
