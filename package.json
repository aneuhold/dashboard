    "storybook": "^8.3.6",
    "svelte": "^5.43.5",
    "svelte-check": "^4.3.3",
    "svelte-confetti-explosion": "^0.4.0",
    "svelty-picker": "^5.2.9",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vitest": "^2.1.3"
  },
  "devDependenciesComments": {
    "svelte": "Locked at 4.x because 5.x doesn't seem to work with @smui components as of 11/25/2023"
  },
  "resolutions": {
    "globals": "^15.3.0"
  },
  "resolutionsComments": {
    "globals": "This is a temporary fix for the globals package due to this issue: https://github.com/eslint/eslint/discussions/17868. Once other packages pull 15+ then should be able to remove this"
  },
  "dependencies": {
    "@sentry/sveltekit": "^10.22.0"
  }
